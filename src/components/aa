최종조작상세일시 DATETIME NOT NULL default current_timestamp() ,
최종조작거래코드 VARCHAR(200) NOT NULL  ,
primary key(수신순번, 수신기준일자),
key `idx_purchased_history_1` (`카드번호`),
key `idx_purchased_history_2` (`카드승인번호`),
key `idx_purchased_history_3` (`카드순번`),
key `idx_purchased_history_4` (`고객번호`),
key `idx_purchased_history_5` (`매입접수번호`),
key `idx_purchased_history_6` (`원매입접수번호`)
);


create table 단위수수료코드관리(
위임수수료코드번호 VARCHAR(10) NOT NULL ,
단위수수료코드번호 VARCHAR(10) NOT NULL ,
단위수수료명 VARCHAR(100) DEFAULT NULL ,
데이터항목명 VARCHAR(100) DEFAULT NULL ,
적용단가 FLOAT DEFAULT NULL ,
수수료기준율 FLOAT DEFAULT NULL ,
수수료증빙구분코드 CHAR(1) DEFAULT NULL ,
수수료자동대사여부 CHAR(1) DEFAULT NULL ,
유효시작일자 CHAR(8) DEFAULT NULL ,
유효종료일자 CHAR(8) DEFAULT NULL ,
수수료청구방식구분코드 CHAR(1) DEFAULT NULL ,
삭제여부 CHAR(1) DEFAULT NULL ,
BC수수료비고내용 VARCHAR(4000) DEFAULT NULL ,
국내외브랜드구분코드 CHAR(1) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key (위임수수료코드번호, 단위수수료코드번호)
);



create table 제신고내역(
변경접수일자 CHAR(8) NOT NULL ,
변경접수일련번호 BIGINT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
변경접수시간 CHAR(6) DEFAULT NULL ,
개인법인구분코드 VARCHAR(2) DEFAULT NULL ,
체크카드회원번호 VARCHAR(10) DEFAULT NULL ,
고객번호 BIGINT(10) UNSIGNED DEFAULT NULL ,
카드순번 BIGINT(10) UNSIGNED DEFAULT NULL ,
카드번호 VARCHAR(50) DEFAULT NULL ,
단말매체구분코드 VARCHAR(2) DEFAULT NULL ,
변경구분코드 VARCHAR(2) DEFAULT NULL ,
변경전내용 VARCHAR(4000) DEFAULT NULL ,
변경후내용 VARCHAR(4000) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
primary key(변경접수일련번호),
key `idx_user_changed_history_1` (`고객번호`),
key `idx_user_changed_history_2` (`카드번호`),
key `idx_user_changed_history_3` (`카드순번`)
);




