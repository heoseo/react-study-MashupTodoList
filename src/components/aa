create table 계정처리내역(
계정처리순번 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
계정처리상세일련번호 BIGINT(20) UNSIGNED NOT NULL ,
계정구분코드 VARCHAR(3) DEFAULT NULL ,
입출구분코드 CHAR(1) DEFAULT NULL ,
계정처리일자 CHAR(8) NOT NULL ,
계정처리일시 CHAR(6) DEFAULT NULL ,
입출금액 BIGINT(20) UNSIGNED DEFAULT NULL ,
카드번호 VARCHAR(50) DEFAULT NULL ,
회계처리구분코드 CHAR(1) DEFAULT NULL ,
가맹점명 VARCHAR(100) DEFAULT NULL ,
계좌번호 VARCHAR(50) DEFAULT NULL ,
거래일자 CHAR(8) DEFAULT NULL ,
거래서비스ID INT(10) UNSIGNED DEFAULT NULL ,
거래ID BIGINT(20) UNSIGNED DEFAULT NULL ,
원거래일자 CHAR(8) DEFAULT NULL ,
원거래서비스ID INT(10) UNSIGNED DEFAULT NULL ,
원거래ID BIGINT(20) UNSIGNED DEFAULT NULL ,
적요유형 VARCHAR(100) DEFAULT NULL ,
전표번호 VARCHAR(8) DEFAULT NULL ,
처리취소여부 CHAR(1) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT null,
primary key (계정처리순번, 계정처리상세일련번호, 계정처리일자)
);


 create table 국내집계내역(
집계일자 CHAR(8) NOT NULL ,
집계구분코드 CHAR(1) NOT NULL ,
집계단위구분코드 CHAR(1) NOT NULL ,
계정구분코드 VARCHAR(3) NOT NULL ,
카드구분코드 CHAR(1) NOT NULL ,
국내외구분코드 CHAR(1) NOT NULL ,
집계기준일자 CHAR(8) DEFAULT NULL ,
출금건수 INT UNSIGNED DEFAULT NULL ,
출금총액 BIGINT UNSIGNED DEFAULT NULL ,
입금건수 INT UNSIGNED DEFAULT NULL ,
입금총액 BIGINT UNSIGNED DEFAULT NULL ,
전체건수 INT UNSIGNED DEFAULT NULL ,
거래총액 BIGINT DEFAULT NULL ,
수수료출금건수 INT UNSIGNED DEFAULT NULL ,
수수료출금총액 BIGINT UNSIGNED DEFAULT NULL ,
수수료입금건수 INT UNSIGNED DEFAULT NULL ,
수수료입금총액 BIGINT UNSIGNED DEFAULT NULL ,
수수료모든건수 INT UNSIGNED DEFAULT NULL ,
수수료합계총액 BIGINT DEFAULT NULL ,
BC대행수수료출금총액 BIGINT UNSIGNED DEFAULT NULL ,
BC대행수수료입금총액 BIGINT UNSIGNED DEFAULT NULL ,
BC대행수수료합계총액 BIGINT DEFAULT NULL ,
가맹점수수료출금총액 BIGINT UNSIGNED DEFAULT NULL ,
가맹점수수료입금총액 BIGINT UNSIGNED DEFAULT NULL ,
가맹점수수료합계총액 BIGINT DEFAULT NULL ,
정산처리여부 CHAR(1) DEFAULT NULL ,
정산처리일시 VARCHAR(14) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key (집계일자, 집계구분코드, 집계단위구분코드, 계정구분코드, 카드구분코드, 국내외구분코드)
);

