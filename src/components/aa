최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key (수신순번, 수신기준일자),
key `idx_cancel_edi_pay_history_1` (`카드번호`),
key `idx_cancel_edi_pay_history_2` (`카드승인번호`),
key `idx_cancel_edi_pay_history_3` (`카드순번`),
key `idx_cancel_edi_pay_history_4` (`고객번호`),
key `idx_cancel_edi_pay_history_5` (`매입접수번호`),
key `idx_cancel_edi_pay_history_6` (`원매입접수번호`)
);


create table 사고접수내역(
변경일자 CHAR(8) NOT NULL ,
변경순번 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
거래고유번호 VARCHAR(12) NOT NULL ,
사고식별번호 VARCHAR(50) DEFAULT NULL ,
사고식별구분코드 CHAR(1) DEFAULT NULL ,
카드사고코드 VARCHAR(4) DEFAULT NULL ,
카드순번 BIGINT(20) UNSIGNED DEFAULT NULL ,
카드번호 VARCHAR(50) DEFAULT NULL ,
사고상태구분코드 CHAR(1) DEFAULT NULL ,
사고등록일자 CHAR(8) DEFAULT NULL ,
사고등록시각 CHAR(6) DEFAULT NULL ,
사고해지일자 CHAR(8) DEFAULT NULL ,
사고해지시각 CHAR(6) DEFAULT NULL ,
직전사고신고순번 BIGINT(20) UNSIGNED DEFAULT NULL ,
직전사고신고일자 CHAR(8) DEFAULT NULL ,
이전처리카드상태코드 CHAR(1) DEFAULT NULL ,
유효기간년월 CHAR(6) DEFAULT NULL ,
신고인명 VARCHAR(60) DEFAULT NULL ,
연락전화번호 VARCHAR(20) DEFAULT NULL ,
고객분실일자 CHAR(8) DEFAULT NULL ,
분실지역명 VARCHAR(60) DEFAULT NULL ,
사고접수번호 CHAR(6) DEFAULT NULL ,
사고접수주체코드 CHAR(1) DEFAULT NULL ,
사고접수구분코드 VARCHAR(2) DEFAULT NULL ,
사고해제구분코드 VARCHAR(2) DEFAULT NULL ,
국외분실여부 CHAR(1) NOT NULL ,
체크카드회원번호 VARCHAR(10) DEFAULT NULL ,
신청직원번호 VARCHAR(50) DEFAULT NULL ,
고객번호 BIGINT(20) UNSIGNED DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key (변경순번),
key `idx_card_accident_receipt_history_1` (`카드번호`),
key `idx_card_accident_receipt_history_2` (`카드순번`),
key `idx_card_accident_receipt_history_3` (`고객번호`),
key `idx_card_accident_receipt_history_4` (`카드사고코드`)
);

create table BIN번호관리(
BIN번호 VARCHAR(8) NOT NULL , 
카드상품코드 VARCHAR(6) NOT NULL , 
채번등록순번 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT, 
브랜드구분코드 CHAR(1) DEFAULT NULL , 
시작카드일련번호 VARCHAR(9) DEFAULT NULL , 
종료카드일련번호 VARCHAR(9) DEFAULT NULL , 
현재카드일련번호 VARCHAR(9) DEFAULT NULL , 
사용여부 CHAR(1) NOT NULL , 
종료일시 CHAR(8) DEFAULT NULL , 
최초조작직원번호 VARCHAR(50) NOT NULL , 
최초조작상세일시 DATETIME NOT NULL default current_timestamp(), 
최초조작거래코드 VARCHAR(200) NOT NULL , 
최종조작직원번호 VARCHAR(50) NOT NULL , 
최종조작상세일시 DATETIME NOT NULL default current_timestamp(), 
최종조작거래코드 VARCHAR(200) NOT NULL , 
primary key (채번등록순번, BIN번호, 카드상품코드)
); 


create table BIN번호이력(
변경순번 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
