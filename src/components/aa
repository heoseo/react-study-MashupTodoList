ISP승인구분코드 CHAR(1) DEFAULT NULL ,
원승인취소시각 CHAR(6) DEFAULT NULL ,
결제계좌번호 VARCHAR(50) DEFAULT NULL ,
카드상품코드 VARCHAR(6) DEFAULT NULL ,
입출금처리코드 CHAR(1) DEFAULT NULL ,
입출금일자 CHAR(8) DEFAULT NULL ,
원거래일자 CHAR(8) DEFAULT NULL ,
원거래서비스ID INT(10) UNSIGNED DEFAULT NULL ,
원거래ID BIGINT(20) UNSIGNED DEFAULT NULL ,
거래일자2 CHAR(8) DEFAULT NULL ,
거래서비스ID INT(10) UNSIGNED DEFAULT NULL ,
거래ID BIGINT(20) UNSIGNED DEFAULT NULL ,
환불처리코드 CHAR(1) DEFAULT NULL ,
환불입출금일자 CHAR(8) DEFAULT NULL ,
