create table 승인거래내역로그(
수신순번 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
수신기준일자 CHAR(8) NOT NULL ,
카드순번 BIGINT(20) UNSIGNED DEFAULT NULL ,
카드번호 VARCHAR(50) DEFAULT NULL ,
승인일자 CHAR(8) DEFAULT NULL ,
카드승인번호 VARCHAR(8) DEFAULT NULL ,
매출종류구분코드 VARCHAR(2) NOT NULL ,
승인처리코드 VARCHAR(6) DEFAULT NULL ,
카드승인금액 BIGINT(20) UNSIGNED DEFAULT NULL ,
승인시간 CHAR(6) DEFAULT NULL ,
승인응답코드 VARCHAR(2) DEFAULT NULL ,
단말기번호 VARCHAR(20) DEFAULT NULL ,
전문관리번호 VARCHAR(12) DEFAULT NULL ,
원장존재여부 CHAR(1) DEFAULT NULL ,
서버파일명 VARCHAR(100) DEFAULT NULL ,
전문전송일자 CHAR(8) DEFAULT NULL ,
전문전송시간 CHAR(6) DEFAULT NULL ,
거래유형코드 VARCHAR(4) DEFAULT NULL ,
고객번호 BIGINT(20) UNSIGNED DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key (수신순번, 수신기준일자),
key `idx_approval_history_log_1` (`카드번호`),
key `idx_approval_history_log_2` (`카드승인번호`),
key `idx_approval_history_log_3` (`카드순번`),
key `idx_approval_history_log_4` (`고객번호`)
);

create table 배치정보 (
배치일련번호 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
배치ID VARCHAR(40) DEFAULT NULL ,
배치명 VARCHAR(40) DEFAULT NULL ,
호출URL VARCHAR(100) DEFAULT NULL ,
선행배치ID VARCHAR(40) DEFAULT NULL ,
실행주기 VARCHAR(20) DEFAULT NULL ,
작업시작시간 VARCHAR(20) DEFAULT NULL ,
비고내용 VARCHAR(1000) DEFAULT NULL ,
서버파일명 VARCHAR(20) DEFAULT NULL ,
등록ID VARCHAR(40) DEFAULT NULL ,
등록일자 VARCHAR(16) DEFAULT NULL ,
수정ID VARCHAR(40) DEFAULT NULL ,
수정일시 VARCHAR(16) DEFAULT NULL ,
primary key(배치일련번호)
);

create table 배치처리내역(
배치일련번호 BIGINT(20) UNSIGNED NOT NULL,
처리일자 CHAR(8) NOT NULL,
처리일련번호 BIGINT(20) UNSIGNED NOT NULL,
시작일시 VARCHAR(16) DEFAULT NULL,
종료일시 VARCHAR(16) DEFAULT NULL,
처리상태 CHAR(1) DEFAULT NULL,
호출구분코드 CHAR(1) DEFAULT NULL,
처리건수 BIGINT(20) UNSIGNED DEFAULT NULL,
대상데이터건수 BIGINT(20) UNSIGNED DEFAULT NULL,
오류메세지 VARCHAR(1000) DEFAULT NULL,
primary key(배치일련번호, 처리일자, 처리일련번호)
);

create table 업종그룹(
업종그룹번호 BIGINT(20) UNSIGNED NOT NULL ,
업종그룹명 varchar(200) DEFAULT NULL ,
비고 varchar(200) DEFAULT NULL ,
최초조작직원번호 varchar(50) NOT NULL ,
최초조작상세일시 datetime NOT NULL DEFAULT CURRENT_TIMESTAMP(),
최초조작거래코드 varchar(200) NOT NULL ,
최종조작직원번호 varchar(50) NOT NULL ,
최종조작상세일시 datetime NOT NULL DEFAULT CURRENT_TIMESTAMP(),
최종조작거래코드 varchar(200) NOT NULL ,
primary key (업종그룹번호),
key `idx_business_group_1` (`업종그룹번호`)
);

create table 업종그룹상세(
업종그룹번호 BIGINT(20) UNSIGNED NOT NULL ,
업종코드 varchar(5) NOT NULL ,
제휴업종대상번호등록일자 CHAR(8) NOT NULL ,
제휴업종대상번호해지일자 CHAR(8) NOT NULL ,
업종그룹명 VARCHAR(200) DEFAULT NULL ,
제휴대상명_업종명 VARCHAR(200) DEFAULT NULL ,
최초조작직원번호 varchar(50) NOT NULL ,
최초조작상세일시 datetime NOT NULL DEFAULT CURRENT_TIMESTAMP(),
최초조작거래코드 varchar(200) NOT NULL ,
최종조작직원번호 varchar(50) NOT NULL ,
최종조작상세일시 datetime NOT NULL DEFAULT CURRENT_TIMESTAMP(),
최종조작거래코드 varchar(200) NOT NULL ,
primary key(업종그룹번호, 업종코드, 제휴업종대상번호등록일자),
key `idx_business_group_detail_1` (`업종코드`)
);

