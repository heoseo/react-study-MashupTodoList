create table 대외송수신로그(
대외기관코드 VARCHAR(4) NOT NULL ,
전문관리번호 BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,
거래일자 CHAR(8) NOT NULL ,
전문거래고유번호 VARCHAR(12) DEFAULT NULL ,
송수신구분코드 VARCHAR(2) DEFAULT NULL ,
거래시분초 CHAR(6) DEFAULT NULL ,
조작거래코드 VARCHAR(10) DEFAULT NULL ,
헤더정보 TEXT(4000) DEFAULT NULL ,
개별공통부정보 TEXT(4000) DEFAULT NULL ,
응답코드 VARCHAR(40) DEFAULT NULL ,
메시지내용 TEXT(8000) DEFAULT NULL ,
카드번호 VARCHAR(50) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key(전문관리번호, 대외기관코드, 거래일자),
key `idx_foreign_sdnrv_log_1` (`카드번호`),
key `idx_foreign_sdnrv_log_2` (`조작거래코드`)
);


create table 그룹가맹점정보(
그룹가맹점번호 VARCHAR(20) NOT NULL ,
제휴대상번호_가맹점번호 VARCHAR(20) NOT NULL ,
