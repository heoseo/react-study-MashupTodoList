계정처리일자 CHAR(8) DEFAULT NULL ,
미입출일련번호 BIGINT(20) UNSIGNED DEFAULT NULL ,
미입출등록일자 CHAR(8) DEFAULT NULL ,
미입출요청여부 CHAR(1) DEFAULT NULL ,
고객번호 BIGINT(20) UNSIGNED DEFAULT NULL ,
면세유공급구분코드 VARCHAR(2) DEFAULT NULL ,
TR번호 VARCHAR(11) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
최초조작상세일시 DATETIME NOT NULL default current_timestamp(),
최초조작거래코드 VARCHAR(200) NOT NULL ,
최종조작직원번호 VARCHAR(50) NOT NULL ,
최종조작상세일시 DATETIME NOT NULL default current_timestamp(),
최종조작거래코드 VARCHAR(200) NOT NULL ,
primary key(거래승인순번, 거래일자),
key `idx_approval_1` (`카드번호`),
key `idx_approval_2` (`카드승인번호`),
key `idx_approval_3` (`카드순번`),
key `idx_approval_4` (`고객번호`)
);

