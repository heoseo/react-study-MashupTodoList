최초카드순번 BIGINT(20) UNSIGNED DEFAULT NULL ,
가맹점번호 VARCHAR(20) DEFAULT NULL ,
가맹점명 VARCHAR(60) DEFAULT NULL ,
최초조작직원번호 VARCHAR(50) NOT NULL ,
